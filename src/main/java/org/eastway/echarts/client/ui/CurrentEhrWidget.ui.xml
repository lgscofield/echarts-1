<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.italics {
			font-style: italic;
		}
		.margin {
			margin: 10px;
		}
		.left {
			float: left;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames='{style.left} {style.margin} {style.italics}' ui:field="container">
		<table>
			<tr>
				<td>Name: <span ui:field="name" /></td>
			</tr>
			<tr>
				<td>DOB: <span ui:field="dob" /> (<span ui:field="age" />)&nbsp;SSN: <span ui:field="ssn" /></td>
			</tr>
			<tr>
				<td>Provider: <span ui:field="provider" />&nbsp;Case Status: <span ui:field="caseStatus" /></td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>

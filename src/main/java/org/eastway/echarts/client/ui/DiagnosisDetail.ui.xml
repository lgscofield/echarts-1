			<tr>
				<td width='20%'><b>Axis 2B</b></td>
				<td><span ui:field='axis2B' /></td>
			</tr>
			<tr>
				<td width='20%'><b>Axis 2C</b></td>
				<td><span ui:field='axis2C' /></td>
			</tr>
			<tr>
				<td width='20%'><b>Axis 3</b></td>
				<td><span ui:field='axis3' /></td>
			</tr>
			<tr>
				<td width='20%'><b>Axis 4</b></td>
				<td><span ui:field='axis4' /></td>
			</tr>
			<tr>
				<td width='20%'><b>Current GAF</b></td>
				<td><span ui:field='currentGAF' /></td>
			</tr>
			<tr>
				<td width='20%'><b>Highest GAF</b></td>
				<td><span ui:field='highestGAF' /></td>
			</tr>
		</table>
	</g:HTMLPanel>

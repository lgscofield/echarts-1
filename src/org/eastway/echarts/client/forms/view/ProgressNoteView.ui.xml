<?xml version="1.0"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		h1 {
			font-size: 110%;
		}

		.margin {
			margin-top: 10px;
			margin-right: 10px;
			margin-bottom: 10px;
		}

		.label {
			margin-top: 4px;
			margin-right: 10px;
		}

		.box {
			border: 1px solid LightGray;
		}
	</ui:style>
	<g:TabLayoutPanel ui:field='tabPanel' barUnit='PX'
		barHeight='0'>
		<g:tab>
			<g:header>Step 1</g:header>
			<g:HTMLPanel>
				<g:HTMLPanel>
					<h1>Progress
						Notes &mdash;
						<span ui:field="patientId" />
					</h1>
					Billing Information
					<br />
					<hr />
					<g:HorizontalPanel addStyleNames='{style.margin}'>
						<g:Label addStyleNames='{style.label}'>Service Date</g:Label>
						<g:TextBox addStyleNames='{style.box}' name="Date" />
					</g:HorizontalPanel>
					<g:HorizontalPanel addStyleNames='{style.margin}'>
						<g:Label addStyleNames='{style.label}'>Service Code</g:Label>
						<g:ListBox addStyleNames='{style.box}' ui:field="serviceCodesListBox" />
					</g:HorizontalPanel>
					<g:HorizontalPanel addStyleNames='{style.margin}'>
						<g:HTMLPanel>
							<g:HorizontalPanel addStyleNames='{style.margin}'>
								<g:Label addStyleNames='{style.label}'>Start Time</g:Label>
								<g:TextBox addStyleNames='{style.box}' name='startTime' />
							</g:HorizontalPanel>
							<g:HorizontalPanel addStyleNames='{style.margin}'>
								<g:Label addStyleNames='{style.label}'>Duration</g:Label>
								<g:TextBox addStyleNames='{style.box}' name='duration' />
							</g:HorizontalPanel>
						</g:HTMLPanel>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:HTMLPanel>
							Location
							<select name="Location1">
								<option value="" />
								<option value="ARS">ARS</option>
								<option value="ECO">ECO</option>
								<option value="EES">EES</option>
								<option value="EJC">EJC</option>
								<option value="ESM">ESM</option>
								<option value="FCO">FCO</option>
								<option value="HEN">HEN</option>
								<option value="LOD">LOD</option>
								<option value="MAR">MAR</option>
								<option value="NOR">NOR</option>
								<option value="WSA">WSA</option>
							</select>
							<Select name="Location2">
								<Option value="" />
								<Option value="11">11</Option>
								<Option value="12">12</Option>
								<Option value="21">21</Option>
								<Option value="22">22</Option>
								<Option value="23">23</Option>
								<Option value="24">24</Option>
								<Option value="31">31</Option>
								<Option value="32">32</Option>
								<Option value="33">33</Option>
								<Option value="34">34</Option>
								<Option value="51">51</Option>
								<Option value="53">53</Option>
								<Option value="54">54</Option>
								<Option value="55">55</Option>
								<Option value="56">56</Option>
								<Option value="61">61</Option>
								<Option value="62">62</Option>
								<Option value="71">71</Option>
							</Select>
							Cost Center:
							<select name="Program">
								<option value="" />
								<option value="071">ARA</option>
								<option value="072">ARB</option>
								<option value="073">ARC</option>
								<option value="074">ARD</option>
								<option value="078">ARG</option>
								<option value="079">ARL</option>
								<option value="075">ART</option>
								<option value="076">ACT</option>
								<option value="077">ACC</option>
								<option value="095">EES</option>
								<option value="064">RCR</option>
								<option value="063">RRC</option>
								<option value="061">RSH</option>
								<option value="062">RTH</option>
								<option value="021">YRO</option>
								<option value="075">ART</option>
								<option value="076">ACT</option>
								<option value="077">ACC</option>
								<option value="095">EES</option>
								<option value="064">RCR</option>
								<option value="063">RRC</option>
								<option value="061">RSH</option>
								<option value="062">RTH</option>
								<option value="021">YRO</option>
								<option value="030">YRR</option>
							</select>
							HIPAA:
							<select name="Mod1">
								<option value="" />
								<option value="HE">HE</option>
								<option value="GT">GT</option>
							</select>
							HIPAA2:
							<select name="Mod2">
								<option value="" />
								<option value="">Blank</option>
								<option value="UK">UK</option>
							</select>
						</g:HTMLPanel>
					</g:HorizontalPanel>
				</g:HTMLPanel>
				<g:Button ui:field='next'>Next</g:Button>
			</g:HTMLPanel>
		</g:tab>
		<g:tab>
			<g:header>Step 2</g:header>
			<g:ScrollPanel height='100%'>
				<g:FlowPanel>
					<g:FlowPanel ui:field="progressNoteBody" />
					<g:Button ui:field='previous'>Previous</g:Button>
					<g:Button ui:field='finish'>Finish</g:Button>
				</g:FlowPanel>
			</g:ScrollPanel>
		</g:tab>
	</g:TabLayoutPanel>
</ui:UiBinder>

<?xml version="1.0"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
	.important {
		font-weight: bold;
	}
  </ui:style>
  <g:VerticalPanel>
    <g:HTMLPanel>
      <div>
        <div>
          <p>
            <h2>Progress
                Notes &mdash;
                <span class="{style.important}" ui:field="nameSpan"/>
              </h2>
            <table width="900" cellspacing="1" border="1" cellpadding="0">
              <tr>
                <td>Progress Notes</td>
              </tr>
              <tr>
                <td>
                    Service Date:
                    <br/>
                    <input type="text" name="Date" size="8" value=""/>
                  </td>
                <td>
                    Service Code:
                    <br/>
                    <g:ListBox ui:field="serviceCodesListBox"/>
                  </td>
                <td>
                    Start Time/Duration:
                    <br/>
                    <input type="text" name="BeginTime" size="7" value="" maxLength="8"/>
                    for
                    <input type="text" name="Minutes" size="2" value="" maxLength="3"/>
                    minutes
                  </td>
                <td>
                    Location:
                    <br/>
                    <select name="Location1"><option value=""/><option value="ARS">ARS</option><option value="ECO">ECO</option><option value="EES">EES</option><option value="EJC">EJC</option><option value="ESM">ESM</option><option value="FCO">FCO</option><option value="HEN">HEN</option><option value="LOD">LOD</option><option value="MAR">MAR</option><option value="NOR">NOR</option><option value="WSA">WSA</option></select>
                    <Select name="Location2"><Option value=""/><Option value="11">11</Option><Option value="12">12</Option><Option value="21">21</Option><Option value="22">22</Option><Option value="23">23</Option><Option value="24">24</Option><Option value="31">31</Option><Option value="32">32</Option><Option value="33">33</Option><Option value="34">34</Option><Option value="51">51</Option><Option value="53">53</Option><Option value="54">54</Option><Option value="55">55</Option><Option value="56">56</Option><Option value="61">61</Option><Option value="62">62</Option><Option value="71">71</Option></Select>
                  </td>
                <td>
                    Cost Center:
                    <br/>
                    <select name="Program"><option value=""/><option value="071">ARA</option><option value="072">ARB</option><option value="073">ARC</option><option value="074">ARD</option><option value="078">ARG</option><option value="079">ARL</option><option value="075">ART</option><option value="076">ACT</option><option value="077">ACC</option><option value="095">EES</option><option value="064">RCR</option><option value="063">RRC</option><option value="061">RSH</option><option value="062">RTH</option><option value="021">YRO</option><option value="075">ART</option><option value="076">ACT</option><option value="077">ACC</option><option value="095">EES</option><option value="064">RCR</option><option value="063">RRC</option><option value="061">RSH</option><option value="062">RTH</option><option value="021">YRO</option><option value="030">YRR</option></select>
                  </td>
                <td>
                    HIPAA:
                    <br/>
                    <select name="Mod1"><option value=""/><option value="HE">HE</option><option value="GT">GT</option></select>
                  </td>
                <td>
                    HIPAA2:
                    <br/>
                    <select name="Mod2"><option value=""/><option value="">Blank</option><option value="UK">UK</option></select>
                  </td>
              </tr>
            </table>
          </p>
        </div>
      </div>
    </g:HTMLPanel>
    <g:HTML ui:field="progressNoteBody"/>
  </g:VerticalPanel>
</ui:UiBinder>
